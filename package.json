  "dependencies": {
    "axios": "^0.18.1",
    "bootstrap": "^4.5.2",
    "http-proxy-middleware": "^0.19.2",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.14.1",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-bootstrap": "^1.3.0",
    "react-bootstrap-sidebar": "0.0.1",
    "react-ckeditor-component": "^1.1.0",
    "react-dom": "^16.13.1",
    "react-dropdown-tree-select": "^1.20.1",
    "react-images-uploader": "^1.2.0-rc1",
    "react-js-pagination": "^3.0.3",
    "react-progress-button": "^5.1.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.0.1",
    "react-sidenav": "^4.0.6",
    "react-timeout": "^1.2.0",
    "react-tooltip": "^3.11.6",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "tinymce": "4.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0"
  },
  "jest": {
    "testURL": "http://localhost:3000/assetlibrary/library",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "lines": -10,
        "functions": 100
      }
    },
    "automock": false,
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "collectCoverageFrom": [
      "!**/src/index.js",
      "!**/src/apiMock/*.js",
      "!**/src/**.test.{js}",
      "!**/src/**.test.{js}",
      "!**/src/components/App.js",
      "**/src/**.{js}"
    ],
    "collectCoverage": true
  }
}
